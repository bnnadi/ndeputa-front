<% layout('./layout/index.ejs') %>
    <div class="container">
        <div class="card card-login mx-auto mt-5">
            <div class="card-header">Verify Password</div>
            <div class="card-body">
                <form>
                    <div class="form-group"><label for="password-verify-form-username">Password</label><input class="form-control" id="password-verify-form-username" type="password" placeholder="Password"></div>
                    <div class="form-group"><label for="password-verify-form-password">Password Confirm</label><input class="form-control" id="password-verify-form-password" type="password" placeholder="Password Confirm"></div>
                    <a id="password-verify-form-submit" class="btn btn-primary btn-block" href="#">Verify</a>
                </form>
            </div>
        </div>
    </div>